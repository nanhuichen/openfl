    "[haxe]": {
        "editor.formatOnSave": true,
        "files.eol": "\n",
        "files.insertFinalNewline": true
    },
    "codedox": {
        "commentprefix": "    ",
        "commentdescription": "",
        "commentend": "**/",
        "alwaysMultiline": false,
        "paramFormat": "",
        "returnFormat": "",
        "neverAskTemplate": true
    },
    "editor.codeLens": false,
    "editor.insertSpaces": false,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "haxe.configurations": [
        [ "scripts/completion.hxml" ],
        [ "-main", "TestMain", "-cp", "src", "-cp", "test/haxelib/unit/src", "-lib", "munit", "-D", "openfl-unit-testing" ]
    ],
    "haxe.enableServerView": true,
    "lime.projectFile": "test/haxelib/functional/project.xml"
}
